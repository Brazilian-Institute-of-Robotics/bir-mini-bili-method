@ARTICLE{Xiang2018104,
author={Xiang, L. and Chen, G. and Zhang, P. and Lu, X.},
title={A smooth trajectory motion control strategy of underwater welding robot [水下焊接机器人弧线轨迹平顺运动控制策略]},
