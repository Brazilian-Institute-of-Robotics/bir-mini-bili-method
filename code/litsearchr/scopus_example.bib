@CONFERENCE{Santos20131726,
author={Santos, I.H. and Galassi, M. and From, P. and Hsu, L. and Lizarralde, F. and Costa, R. and Freitas, G. and Motta-Ribeiro, G. and Almeida-Antonio, T. and Coutinho, F. and Raposo, A. and Carvalho, F. and Medeiros, D.},
title={A robotics framework for planning the offshore robotizing using virtual reality techniques},
