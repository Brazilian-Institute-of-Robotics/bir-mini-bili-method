@CONFERENCE{Borlaug20198,
author={Borlaug, I.-L.G. and Sverdrup-Thygeson, J. and Pettersen, K.Y. and Gravdahl, J.T.},
title={Combined kinematic and dynamic control of an underwater swimming manipulator},
